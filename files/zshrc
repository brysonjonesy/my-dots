# uninstall by removing these lines or running `tabtab uninstall sls`
[[ -f /Users/rizowski/.config/yarn/global/node_modules/tabtab/.completions/sls.zsh ]] && . /Users/rizowski/.config/yarn/global/node_modules/tabtab/.completions/sls.zsh
# tabtab source for slss package
