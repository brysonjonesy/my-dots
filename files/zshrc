# uninstall by removing these lines or running `tabtab uninstall serverless`
[[ -f /Users/rizowski/.config/yarn/global/node_modules/tabtab/.completions/serverless.zsh ]] && . /Users/rizowski/.config/yarn/global/node_modules/tabtab/.completions/serverless.zsh
